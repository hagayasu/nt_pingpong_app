<h1>Users#edit</h1>
<p>Find me in app/views/users/edit.html.erb</p>



<%= form_for @user do |f| %>

<%= f.label :username %>
<%= f.text_field :username %>

<%= f.label :email %>
<%= f.text_field :email %>

<%= f.label :profile %>
<%= f.text_field :profile %>

<%= f.label :prfile_image %>
<%= f.attachment_field :profile_image, class: "input" %>

<%= f.submit %>

<% end %>
